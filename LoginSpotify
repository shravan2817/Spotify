public class Main {

static WebDriver webDriver;

public static void main(String[] args){
            System.setProperty("webdriver.chrome.driver", "c://chromedriver.exe");
	WebDriver driver=new ChromeDriver();
	driver.get("https://open.spotify.com/");
            webDriver.findElement(By.xpath("//button[text()='Log in']")).click();
driver.findElement(by.cssSelector("input#login-username").click(); driver.findElement(by.cssSelector("input#login-username").sendKeys(" "); driver.findElement(by.cssSelector("input#login-password").click(); driver.findElement(by.cssSelector("input#login-password").sendKeys(" ");

}

}
